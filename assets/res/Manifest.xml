<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<manifest useDirectView="false">
    <log>y</log>
    <lang>ko</lang>
    <resource>
        <base_version>000000</base_version>
        <target>app</target>
        <update>
            <server/>
            <trcode/>
            <mode/>
        </update>
    </resource>
    <startpage>
        <name>www/html/index.html</name>
        <orient>port</orient>
        <orient-tablet>port</orient-tablet>
    </startpage>
    <settings>
        <push>
            <receiver>
                <policy>user</policy>
                <version/>
                <server/>
                <gcm-sender-id/>
                <android-push-type>GCM</android-push-type>
                <use-phone_number>Y</use-phone_number>
                <use-permission>Y</use-permission>
            </receiver>
            <upns>
                <agent-service-type>agent</agent-service-type>
                <agent-classpath>lnc.idccon.</agent-classpath>
                <agent-package-name>lnc.idccon</agent-package-name>
                <agent-server-url/>
                <agent-restart-interval/>
            </upns>
        </push>
    </settings>
    <default_action>
        <orient>port</orient>
        <orient-tablet>port</orient-tablet>
        <animate>slide_left</animate>
        <indicator>n</indicator>
    </default_action>
    <library>
        <ext>lnc.idccon.implementation.ExtendWNInterface</ext>
        <addons>
            <path>m.client.library.addon.db.WNInterfaceDB</path>
            <path>m.client.library.addon.file.WNInterfaceFile</path>
            <path>m.client.library.addon.media.WNInterfaceMedia</path>
            <path>m.client.library.addon.net.WNInterfaceNet</path>
            <path>m.client.library.addon.netext.WNInterfaceNetExt</path>
            <path>m.client.library.addon.popup.WNInterfacePopup</path>
            <path>m.client.library.addon.zip.WNInterfaceZip</path>
            <path>m.client.library.addon.locale.WNInterfaceZip</path>
        </addons>
        <plugins>
            <path>m.client.library.plugin.drawing.WNInterfaceDrawing</path>
            <path>m.client.library.plugin.location.WNInterfaceLocation</path>
            <path>m.client.library.plugin.viewer.WNInterfaceViewer</path>
        	<path>m.client.library.plugin.push.WNInterfacePush</path>
		</plugins>
    </library>
    <network>
        <http/>
        <socket/>
    </network>
    <application>
        <android>
            <hardwareaccelerated>hardware</hardwareaccelerated>
            <user-theme>n</user-theme>
            <use-youtube>n</use-youtube>
        </android>
    </application>
</manifest>
